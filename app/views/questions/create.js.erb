$('.content-wrapper #question-form').hide();
$(".content-wrapper .questiontable tr:first" ).after("<%= j (render 'row') %>")
console.log('here');
$(".content-wrapper" ).append("<%= j (render 'askquestionbutton') %>")

$(document).mousedown(function (e){
   var container = $(".content-wrapper #question-form");
    if (!container.is(e.target) // if the target of the click isn't the container...
        && container.has(e.target).length === 0) // ... nor a descendant of the container
    {
     $('.content-wrapper #ask-question-btn').show();
         
    }
});

$('.content-wrapper #submit-new-question-btn').on( "click", function(){
  console.log("h");
  $(".content-wrapper #question-form").remove();
});
