<!DOCTYPE html>
<html>
  <head>
        <title>Meetups</title>
        <style>
        body {padding: 0; margin: 0 }
        html, body, #map { height: 100% }
        </style>
  </head>

  <body>
        <div id="map"></div>
        <script type ="text/javascript">
        var basemap = L.tileLayer('https://{s}.tiles.mapbox.com/v4/hanieldanson.ld3pa9gn/{z}/{x}/{y}.png',
                                  {     maxZoom: 18     });

        $.getJSON('https://data.cityofchicago.org/resource/wa2i-tm5d.json', function(data) {
                var geojson = L.geoJson(data, {
                  onEachFeature: function (feature, layer) {
                          layer.bindPopup(feature.properties.NAME + '<br />' + feature.properties.ADDRESS);
                  }
                });
        var map = L.map('map').setView([-87.6970, 41.8269], 12);
        basemap.addTo(map);
        geojson.addTo(map);
        });


  </script>
 </body>
</html>
